void plusMinus(vector<int> arr) {
    int n = arr.size();
    float positive=0;
    float negative=0;
    float zero=0;
    for(int i = 0;i<n;i++)
    {
        if(arr[i]>0)
            positive++;
        else if(arr[i]<0)
            negative++;
        else{
            zero++;
        }
    }
    cout << positive/n <<endl;
    cout << negative/n <<endl;
    cout << zero/n << endl;
}
